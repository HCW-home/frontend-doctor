{
  "compileOnSave": false,
  "compilerOptions": {
    "paths": {
      "rxjs/*": [
        "../node_modules/rxjs/*"
      ]
    },
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "esModuleInterop": true,
    "useDefineForClassFields": false
  },
  "angularCompilerOptions":{
    "enableIvy":true
  }
}
